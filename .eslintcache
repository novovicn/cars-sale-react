Props)(App);\n","C:\\Users\\Nenad\\Desktop\\used-cars\\used-cars-sale\\src\\reportWebVitals.js",[],"C:\\Users\\Nenad\\Desktop\\used-cars\\used-cars-sale\\src\\Header.js",["98"],"import React from \"react\";\r\nimport \"./Header.css\";\r\nimport logo from \"./img/logobmw1.png\";\r\nimport { connect } from 'react-redux';\r\nimport { Link } from \"react-router-dom\";\r\nimport { auth } from \"./firebase\";\r\n\r\nfunction Header(props) {\r\n\r\n  const userLogout = () => {\r\n    if(props.user){\r\n      auth.signOut();\r\n      props.onLogUserOut(null);\r\n    }\r\n  }\r\n\r\n  return (\r\n    <div className=\"header\">\r\n      <div className=\"header__firstRow\">\r\n        <img className=\"header__logo\" src={logo} />\r\n        <p>Hello, {props.user? props.user : \"guest.\"}</p>\r\n        <div className=\"header__auth\">\r\n          <Link to={!props.user && \"/auth\"}>\r\n            <button onClick={userLogout} className=\"header__authBtn\">\r\n              {props.user ? \"Logout\" : \"Login\"}\r\n            </button>\r\n          </Link>\r\n        </div>\r\n      </div>\r\n      <div className=\"header__secondRow\">\r\n        <Link to=\"/\">Home</Link>\r\n        <Link to=\"/sellcar\">Sell car</Link>\r\n        <Link to=\"/findcar\">Find car</Link>\r\n        <Link to=\"/about\">About</Link>\r\n        <Link to=\"/contact\">Contact</Link>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nconst mapStateToProps = state => {\r\n  return{\r\n    user: state.user\r\n  };\r\n}\r\n\r\nconst mapDispatchToProps = dispatch => {\r\n  return {\r\n    onLogUserOut: (authUser) => dispatch({type: 'SET_USER', user: authUser}),\r\n  }\r\n}\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(Header);\r\n","C:\\Users\\Nenad\\Desktop\\used-cars\\used-cars-sale\\src\\Landing.js",["99"],"import React from \"react\";\r\nimport \"./Landing.css\";\r\nimport newsImg from \"./img/car-news.jpg\";\r\n\r\nfunction Landing() {\r\n  return (\r\n    <div className=\"landing